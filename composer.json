{
    "name": "mhassan654/licensing-connector",
    "description": "Laravel connector for licensing server package",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "mockery/mockery": "1.4",
        "orchestra/testbench": "7.0",
        "phpunit/phpunit": "9.5"
    },
    "license": "MIT",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mhassan654\\LicensingConnector\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Muwonge Hassan",
            "email": "hassansaava@gmail.com"
        }
    ],

    "minimum-stability": "stable"
